{
  "name": "automobile-crawler",
  "version": "1.0.0",
  "description": "Content parser for collecting basic data on car sales from popular categories.",
  "main": "./src/server.js",
  "type": "module",
  "author": "Aleksey Stepin",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node ./src/server.js",
    "start:pm2": "pm2 start ./src/server.js -- --port 8080 --name content_parser",
    "start:stage:pm2": "pm2 start ./src/server.js --name content_parser",
    "restart:stage:pm2": "pm2 restart content_parser",
    "stop:pm2": "pm2 stop content_parser",
    "dev": "nodemon ./src/server.js",
    "lint": "eslint . --ext .js",
    "fix": "npx eslint --fix .",
    "test": "jest",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:example": "npx sequelize-cli migration:generate --name create_car_table"
  },
  "dependencies": {
    "@adminjs/fastify": "^4.1.3",
    "@adminjs/sequelize": "^4.1.1",
    "@adminjs/themes": "^1.0.1",
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/postgres": "^5.2.2",
    "@fastify/redis": "^7.0.1",
    "@fastify/session": "^10.9.0",
    "@fastify/static": "^7.0.0",
    "@fastify/swagger": "^8.15.0",
    "adminjs": "^7.8.12",
    "connect-pg-simple": "^10.0.0",
    "fastify": "^4.28.1",
    "pg": "^8.12.0",
    "puppeteer": "^23.3.0",
    "sequelize": "^6.37.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "nodemon": "^3.1.4"
  }
}
